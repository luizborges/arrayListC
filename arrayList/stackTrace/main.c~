#include "stackTrace.h"



////////////////////////////////////////////////////////////////////////////////
// Test Functions Head
////////////////////////////////////////////////////////////////////////////////
void test1();

////////////////////////////////////////////////////////////////////////////////
// Functions for test
////////////////////////////////////////////////////////////////////////////////
/* A dummy function to make the backtrace more interesting. */
int func1 (void){ return 1; }
int func2 (void){ return 2; }
int func3 (void){ return 3; }
int func4 (void){ return 4; }


////////////////////////////////////////////////////////////////////////////////
// Main
////////////////////////////////////////////////////////////////////////////////
int main (void) {
  StackTrace_Init();
  test1();
  return 1;
}

////////////////////////////////////////////////////////////////////////////////
// Test Functions Body
////////////////////////////////////////////////////////////////////////////////
void test1() {
	func1();
	func2();
	func3();
	func4();
	abort();
}








